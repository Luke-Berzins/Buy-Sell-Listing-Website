<!DOCTYPE html>
<html lang="en">
  <%- include('partials/_head') %>
  <body>
    <%- include('partials/_navbar') %>
    <h1>Conversations!</h1>
    <% let counter = 0;
    for(let conversation of conversations) {
      counter++; %>
      <tr>
        <h3>Convo <%= counter %></h1>
        <td>convo id <%= conversation['id'] %></td>
        <td> seller id <%= conversation['seller_id'] %></td>
        <td>buyer id <%= conversation['buyer_id'] %></td>
        <td>posting id <%= conversation['posting_id'] %></td>
        <br>
        <br>
      </tr>
    <% } %>
  </body>
</html>
